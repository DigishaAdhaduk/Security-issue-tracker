generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(uuid())
  email String @unique
  pass  String
  name  String?
}

model Issue {
  id    String @id @default(uuid())
  type  String
  title String
  desc  String
  uid   String
}

model Token {
  id  String @id @default(uuid())
  val String
  uid String
}
